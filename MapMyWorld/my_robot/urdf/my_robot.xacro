<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
  <xacro:property name="body_mass" value="10"/>
  <xacro:property name="body_length" value="0.8"/>
  <xacro:property name="body_width" value="0.3"/>
  <xacro:property name="body_height" value="0.2"/>
  <xacro:property name="wheel_mass" value="5"/>
  <xacro:property name="wheel_radius" value="0.2"/>
  <xacro:property name="wheel_length" value="0.1"/>

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.2 0 0 0</pose>

    <inertial>
      <mass value="${body_mass}"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${body_mass*(body_length*body_length + body_height*body_height)/12}" ixy="0" ixz="0"
          iyy="${body_mass*(body_width*body_width + body_height*body_height)/12}" iyz="0"
          izz="${body_mass*(body_length*body_length + body_width*body_width)/12}"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size="${body_length} ${body_width} ${body_height}"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="${body_length} ${body_width} ${body_height}"/>
      </geometry>
    </visual>
  </link>

  <link name='front_left_wheel'>
    <collision name='front_left_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </collision>
    <visual name='front_left_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="${wheel_mass}"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" ixy="0" ixz="0"
          iyy="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" iyz="0"
          izz="${wheel_mass*wheel_radius*wheel_radius/2}"
      />
    </inertial>
  </link>

  <link name='front_right_wheel'>
    <collision name='front_right_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </collision>
    <visual name='front_right_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="${wheel_mass}"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" ixy="0" ixz="0"
          iyy="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" iyz="0"
          izz="${wheel_mass*wheel_radius*wheel_radius/2}"
      />
    </inertial>
  </link>

  <link name='back_left_wheel'>
    <collision name='back_left_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </collision>
    <visual name='back_left_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="${wheel_mass}"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" ixy="0" ixz="0"
          iyy="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" iyz="0"
          izz="${wheel_mass*wheel_radius*wheel_radius/2}"
      />
    </inertial>
  </link>

  <link name='back_right_wheel'>
    <collision name='back_right_wheel_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </collision>
    <visual name='back_right_wheel_visual'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="${wheel_length}"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="${wheel_mass}"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" ixy="0" ixz="0"
          iyy="${wheel_mass*(3*wheel_radius*wheel_radius + wheel_length*wheel_length)/12}" iyz="0"
          izz="${wheel_mass*wheel_radius*wheel_radius/2}"
      />
    </inertial>
  </link>


  <joint type="continuous" name="front_left_wheel_hinge">
    <origin xyz="0.25 0.23 0" rpy="0 0 0"/>
    <child link="front_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="front_right_wheel_hinge">
    <origin xyz="0.25 -0.23 0" rpy="0 0 0"/>
    <child link="front_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="back_left_wheel_hinge">
    <origin xyz="-0.25 0.23 0" rpy="0 0 0"/>
    <child link="back_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="back_right_wheel_hinge">
    <origin xyz="-0.25 -0.23 0" rpy="0 0 0"/>
    <child link="back_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>


  <link name='camera'>
    <collision name='camera_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size = "0.05 0.05 0.05"/>
      </geometry>
    </collision>
    <visual name='camera_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.05 0.05 0.05" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
  </link>
  
  <link name="camera_link_optical">
  </link>
  
  <joint type="fixed" name="camera_optical_joint">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
    <child link="camera_link_optical"/>
    <parent link="camera"/>
  </joint>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.4 0 0" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>

  <link name='hokuyo'>
    <collision name='hokuyo_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size = "0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <visual name='hokuyo_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh  filename ="package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
  </link>

  <link name='hokuyo_holder'>
    <collision name='holder_collision'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size = "0.2 0.2 0.1"/>
      </geometry>
    </collision>
    <visual name='holder_visual'>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size = "0.2 0.2 0.1"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
  </link>

  <joint type="fixed" name="hokuyo_holder_joint">
    <origin xyz="0.3 0 0.15" rpy="0 0 0"/>
    <child link="hokuyo_holder"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>



  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0.0 0 0.08" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="hokuyo_holder"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
  </joint>



</robot>